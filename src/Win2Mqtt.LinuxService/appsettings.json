{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "Win2MQTT": {
    "Broker": {
      "Server": "homeassistant.local",
      "Port": 1883,
      "Username": "win2mqtt",
      "Password": "win2mqtt"
    },
    "DeviceIdentifier": "sembo-ubuntu",
    "MqttTopicQoS": 1,
    "TimerInterval": 5,
    "Sensors": {
      "Timestamp": {
        "enabled": true
      }
    },
    "MultiSensors": 
    {
        
    },
    "Listeners": {
      "StartProcess": {
        "Topic": "startprocess",
        "Enabled": true
      },
      "GetProcesses": {
        "Topic": "getprocesses",
        "Enabled": true
      }
    }
  }
}
