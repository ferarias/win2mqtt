{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Warning"
            }
        }
    },
    "Win2MQTT": {
        "Broker": {
            "Server": "192.168.1.4",
            "Port": 1883,
            "Username": "",
            "Password": ""
        },
        "MachineIdentifier": "lechuck",
        "MqttTopicQoS": 1,
        "TimerInterval": 5,
        "EnableNotifications": false,
        "Sensors": {
            "TimestampSensor": {
                "enabled": true,
                "topic": "timestamp"
            },
            "CpuProcessorTimeSensor": {
                "enabled": true,
                "topic": "cpuprocessortime"
            },
            "FreeMemorySensor": {
                "enabled": true,
                "topic": "freememory"
            },
            "NetworkAvailabilitySensor": {
                "enabled": true,
                "topic": "networkavailability"
            },
            "ComputerInUseSensor": {
                "enabled": false,
                "topic": "computerinuse"
            }
        },
        "MultiSensors": {
            "DriveMultiSensor": {
                "enabled": true,
                "topic": "drive",
                "Sensors": {
                    "DriveFreeSizeSensor": {
                        "enabled": true,
                        "topic": "sizefree"
                    },
                    "DrivePercentFreeSizeSensor": {
                        "enabled": true,
                        "topic": "percentfree"
                    },
                    "DriveTotalSizeSensor": {
                        "enabled": true,
                        "topic": "sizetotal"
                    }
                }
            }
        },
        "Listeners": {
            "Hibernate": {
                "Topic": "hibernate",
                "Enabled": true
            },
            "Shutdown": {
                "Topic": "shutdown",
                "Enabled": true
            },
            "Reboot": {
                "Topic": "reboot",
                "Enabled": true
            },
            "Suspend": {
                "Topic": "suspend",
                "Enabled": true
            },

            "SendNotification": {
                "Topic": "sendnotification",
                "Enabled": true
            },
            "StartProcess": {
                "Topic": "startprocess",
                "Enabled": true
            },
            "GetProcesses": {
                "Topic": "getprocesses",
                "Enabled": true
            }


        }
    }
}
