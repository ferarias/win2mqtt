{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Warning"
            }
        }
    },
    "Win2MQTT": {
        "Broker": {
            "Server": "192.168.1.4",
            "Port": 1883,
            "Username": "",
            "Password": ""
        },
        "MachineIdentifier": "lechuck",
        "MqttTopicQoS": 1,
        "TimerInterval": 5,
        "EnableNotifications": false,
        "Sensors": {
            "TimeSensor": true,
            "CpuSensor": true,
            "DiskSensor": true,
            "MemorySensor": true,
            "NetworkSensor": true,
            "ComputerInUseSensor": true
        },
        "Listeners": {
            "Hibernate": {
                "Topic": "hibernate",
                "Enabled": true
            },
            "Shutdown": {
                "Topic": "shutdown",
                "Enabled": true
            },
            "Reboot": {
                "Topic": "reboot",
                "Enabled": true
            },
            "Suspend": {
                "Topic": "suspend",
                "Enabled": true
            },

            "SendNotification": {
                "Topic": "sendnotification",
                "Enabled": true
            },
            "StartProcess": {
                "Topic": "startprocess",
                "Enabled": true
            },
            "GetProcesses": {
                "Topic": "getprocesses",
                "Enabled": true
            }


        }
    }
}
