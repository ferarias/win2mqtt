{
    "Win2MQTT": {
        "Broker": {
            "Server": "localhost",
            "Port": 1883,
            "Username": "win2mqtt",
            "Password": "win2mqtt"
        },
        "DeviceIdentifier": "my-linux-machine",
        "MqttTopicQoS": 1,
        "TimerInterval": 5,
        "Sensors": {
            "Timestamp": {
                "enabled": true
            },
            "CpuProcessorTime": {
                "enabled": true
            },
            "FreeMemory": {
                "enabled": true
            },
            "NetworkAvailability": {
                "enabled": true
            },
            "ComputerInUse": {
                "enabled": false
            }
        },
        "MultiSensors": {
            "Drives": {
                "enabled": true,
                "Sensors": {
                    "DriveFreeSize": {
                        "enabled": true
                    },
                    "DrivePercentFreeSize": {
                        "enabled": true
                    },
                    "DriveTotalSize": {
                        "enabled": true
                    }
                }
            }
        },
        "Listeners": {
            "Hibernate": {
                "Topic": "hibernate",
                "Enabled": true
            },
            "Reboot": {
                "Topic": "reboot",
                "Enabled": true
            },
            "Shutdown": {
                "Topic": "shutdown",
                "Enabled": true
            },
            "Suspend": {
                "Topic": "suspend",
                "Enabled": true
            },
            "SendNotification": {
                "Topic": "sendnotification",
                "Enabled": true
            },
            "StartProcess": {
                "Topic": "startprocess",
                "Enabled": true
            },
            "KillProcess": {
                "Topic": "killprocess",
                "Enabled": true
            },
            "GetProcesses": {
                "Topic": "getprocesses",
                "Enabled": true
            },
            "GetProcess": {
                "Topic": "getprocess",
                "Enabled": true
            }
        }
    }
}
